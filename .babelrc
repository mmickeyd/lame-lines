{ "presets": [ "@babel/preset-env", [
  "@babel/preset-react", {
    "pragma": "dom",
    "pragmaFrag": "DomFrag",
    "throwIfNamespace": false
    }
  ]
],
  "plugins": [
    ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    [ "@babel/plugin-transform-react-jsx", {
      // "pragma": "dom",
      "throwIfNamespace": false
      }
    ]
  ]
}